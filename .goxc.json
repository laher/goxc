{
	"AppName": "goxc",
	"ArtifactsDest": "../goxc-pages/dl",
	"Tasks": [
		"interpolate-source",
		"go-fmt",
		"default",
		"deb-source"
	],
	"ResourcesInclude": "README.md,.\\_test1/*,_test2",
	"ResourcesExclude": "*.go",
	"PackageVersion": "0.16.1",
	"TaskSettings": {
		"archive-zip": {},
		"bintray": {
			"downloadspage": "bintray.md",
			"package": "goxc",
			"repository": "utils",
			"subject": "laher"
		},
		"codesign": {
			"id": "goxc"
		},
		"debs": {
			"metadata": {
				"description": "Cross-compiler utility for Go",
				"maintainer": "Am Laher (https://github.com/laher)"
			},
			"metadata-deb": {
				"Depends": "golang",
				"Homepage": "https://github.com/laher/goxc"
			},
			"other-mapped-files": {
				"/": "deb-contents/"
			}
		},
		"downloads-page": {
			"filename": "index.md",
			"templateFile": "downloads.tpl"
		},
		"publish-github": {
			"owner": "laher",
			"repository": "goxc"
		},
		"go-test": {
			"short": true,
			"i": true
		},
		"xc": {
			"test-setting": "test-value"
		}
	},
	"ConfigVersion": "0.9",
	"BuildSettings": {
		"LdFlags": "-s",
		"LdFlagsXVars": {
			"TimeNow": "main.BUILD_DATE",
			"Version": "main.VERSION"
		}
	},
	"Env": [
		"GOPATH=Godeps{{.PLS}}{{.Env.GOPATH}}"
	]
}
